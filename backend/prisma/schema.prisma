generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WorkerScheduler {
  id        Int      @id @default(autoincrement())
  nextFetch DateTime
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model EstadoPacienteCount {
  id        Int      @id @default(autoincrement())
  estado    String   @unique
  count     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model VacinaNomeCount {
  id        Int      @id @default(autoincrement())
  nome      String   @unique
  total     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model IdadePacienteCount {
  id        Int      @id @default(autoincrement())
  idade     Int      @unique
  total     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model SistemaOrigemCount {
  id        Int      @id @default(autoincrement())
  sistema   String   @unique
  total     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model SexoCount {
  id        Int      @id @default(autoincrement())
  sexo      String   @unique
  total     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model CategoriaCount {
  id        Int      @id @default(autoincrement())
  categoria String   @unique
  total     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model RacaCount {
  id        Int      @id @default(autoincrement())
  nome      String   @unique
  total     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model VacinaDoseCount {
  id        Int      @id @default(autoincrement())
  descricao String   @unique
  total     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model PaisPacienteCount {
  id        Int      @id @default(autoincrement())
  nome      String   @unique
  total     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model FabricanteCount {
  id             Int      @id @default(autoincrement())
  fabricanteNome String   @unique
  total          Int
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}

model MunicipioCount {
  id   Int    @id @default(autoincrement())
  nome String

  estadoId Int
  estado   Estado @relation(fields: [estadoId], references: [id])

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Estado {
  id        Int              @id @default(autoincrement())
  nome      String
  municipio MunicipioCount[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
